{
	"name": "cfExtension",
	"displayName": "cfExtension",
	"description": "Codeforces Integrator ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cfExtension.helloWorld",
		"onCommand:cfExtension.login",
		"onCommand:cfExtension.logout",
		"onCommand:cfExtension.fetchProblem",
		"onCommand:cfExtension.fetchContest",
		"onCommand:cfExtension.showproblem"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"editor/title": [
				{
					"when": "resourceScheme == file && resourceExtname == .cpp",
					"command": "cfExtension.showproblem",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
				"command": "cfExtension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "cfExtension.login",
				"title": "User Login"
			},
			{
				"command": "cfExtension.logout",
				"title": "Logout"
			},
			{
				"command": "cfExtension.fetchProblem",
				"title": "Problem-url"
			},
			{
				"command": "cfExtension.fetchContest",
				"title": "Contest-url"
			},
			{
				"command": "cfExtension.showproblem",
				"title": "Show Problem"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"cheerio": "^1.0.0-rc.9",
		"path": "^0.12.7",
		"puppeteer": "^9.1.1",
		"querystring": "^0.2.1"
	}
}
